{
  "name": "evm-scaffolding-cli",
  "version": "2.0.02",
  "description": "A CLI frontend repo for EVM scaffolding that allows users to choose dependencies to install and modifies scripts based on user input.",
  "main": "dist/index.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "node dist/index.js",
    "test": "jest tests/index.test.js",
    "lint": "yarn rome check src --apply",
    "format": "yarn rome format src --write",
    "lintCheck": "yarn rome check src",
    "tsc": "tsc",
    "publish": "npm publish",
    "ts-prune": "ts-prune"
  },
  "bin": {
    "evm-scaffolding-cli": "./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seranged/evm-scaffolding-cli.git"
  },
  "keywords": [
    "EVM",
    "Seranged",
    "frontend",
    "ETH"
  ],
  "author": "Seranged",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seranged/evm-scaffolding-cli/issues"
  },
  "homepage": "https://github.com/seranged/evm-scaffolding-cli#readme",
  "dependencies": {
    "@types/inquirer": "^9.0.3",
    "inquirer": "^9.2.12",
    "latest-version": "^7.0.0",
    "lodash": "^4.17.21",
    "ora": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rome": "^12.1.3",
    "sinon": "^17.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/lodash": "^4.14.197",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.5.8",
    "@types/react": "^18.2.21",
    "@types/sinon": "^17.0.2",
    "eslint": "^8.56.0",
    "ncp": "^2.0.0",
    "prettier": "^3.1.1",
    "ts-prune": "^0.10.3",
    "typescript": "^5.2.2"
  }
}
